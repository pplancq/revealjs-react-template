{
  "name": "revealjs-react-template",
  "version": "0.1.0",
  "license": "MIT",
  "description": "revealjs-react-template",
  "private": true,
  "type": "module",
  "packageManager": "npm@11.8.0",
  "scripts": {
    "start": "rsbuild dev",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "lint": "node scripts/lint.js",
    "lint:eslint": "eslint \"{public,src}/**/*.{js,jsx,mjs,ts,tsx}\"",
    "lint:eslint:fix": "eslint \"{public,src}/**/*.{js,jsx,mjs,ts,tsx}\" --fix",
    "lint:stylelint": "stylelint \"src/**/*.{scss,css}\"",
    "lint:stylelint:fix": "stylelint \"src/**/*.{scss,css}\" --fix",
    "lint:prettier": "prettier \"{public,src}/**/*.!(js|jsx|mjs|ts|tsx|scss|css)\" --check --ignore-unknown",
    "lint:prettier:fix": "prettier \"{public,src}/**/*.!(js|jsx|mjs|ts|tsx|scss|css)\" --write --ignore-unknown",
    "lint:tsc": "tsc --noEmit",
    "package:check": "npm exec --yes package-lock-utd@1.1.3",
    "prepare": "husky"
  },
  "homepage": "https://pplancq.github.io/revealjs-react-template",
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "reveal.js": "^5.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@pplancq/commitlint-config": "^3.0.1",
    "@pplancq/eslint-config": "^6.0.1",
    "@pplancq/postcss-config": "^3.0.2",
    "@pplancq/prettier-config": "^2.0.0",
    "@pplancq/stylelint-config": "^5.0.0",
    "@rsbuild/core": "^1.7.3",
    "@rsbuild/plugin-eslint": "^1.2.1",
    "@rsbuild/plugin-react": "^1.4.5",
    "@rsbuild/plugin-sass": "^1.5.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/reveal.js": "^5.2.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "stylelint": "^17.1.1",
    "stylelint-prettier": "^5.0.3",
    "tsc-files": "^1.1.4",
    "typescript": "^5.9.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
